package array03;

import java.util.Scanner;

public class P_1210 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		// test case
		int tc = 10;
		for (int t = 1; t <= tc; t++) {

			// 범위 초과를 방지하기 위해 배열 크기를 좌우 2만큼 크게 생성
			int[][] arr = new int[102][102];
			int dx = 0; // 도착점의 행 index
			int dy = 0; // 도착점의 열 index
			sc.nextInt();
			// 배열 값 입력
			for (int i = 1; i <= 100; i++) {
				for (int j = 1; j <= 100; j++) {
					arr[i][j] = sc.nextInt();
					if (arr[i][j] == 2) {
						dx = i;
						dy = j;
					}
				}
			}

			// ed를 기준으로 위로 탐색하되, 중간에 왼쪽이나 오른쪽에 1이 있을 경우 방향 전환
			// 좌우 1 찾고 없으면 위의 1로 올라간다
			// 시작점은 ed이다.
			while (true) {
				dx--;
				while (arr[dx][dy - 1] == 0 && arr[dx][dy + 1] == 0 && dx > 1) {
					dx--;
				}
				if (dx == 1) {
					break;
				}
				if (arr[dx][dy - 1] != 0) { // 왼쪽 전진
					while (arr[dx][dy - 1] != 0)
						dy--;
				} else if (arr[dx][dy + 1] != 0) { // 오른쪽 전진
					while (arr[dx][dy + 1] != 0)
						dy++;
				}

			}
			System.out.printf("#%d %d\n", t, dy-1);
		}
	}
}
