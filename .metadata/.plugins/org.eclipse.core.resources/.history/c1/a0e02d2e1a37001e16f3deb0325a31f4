package array04;

import java.util.Arrays;

public class Array04_03_배열출력 {
	static int[][] arr = { { 1, 2, 3 }, { 4, 5, 6, }, { 7, 8, 9, } };
	static int n = arr.length;

	public static void main(String[] args) {

		// 전치행렬
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				int tmp = arr[i][j];
				arr[i][j] = arr[j][i];
				arr[j][i] = tmp;
			}
		}

	}
}	
