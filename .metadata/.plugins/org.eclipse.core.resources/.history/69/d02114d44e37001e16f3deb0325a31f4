package array04;

import java.util.Scanner;

public class P_11315 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		// test case
		int tc = sc.nextInt();
		for (int t = 1; t <= tc; t++) {
		int n = sc.nextInt();
		String[] sarr = new String[n];
		for (int i = 0; i < n; i++) {
			sarr[i] = sc.next();
		}
		
		for (int i = 0; i < n; i++) {
			sarr[i].toCharArray();
		}
	

		System.out.printf("#%d", t);
		}
	}
}
