import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int tc = sc.nextInt();
		
		for (int t = 1; t <= tc; t++) {
			int a = sc.nextInt();
			int b = sc.nextInt();
			
			int data = (int) Math.pow(a, b);
			int ans = data % 10;
			
			if (ans == 0) {
				ans = 10;
			}
			System.out.println(ans);
		}
		
	}
}
