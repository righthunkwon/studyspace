
public class Array01_01_배열순회 {
	public static void main(String[] args) {
		int[] nums = { 0, 1, 2, 3, 4, 5, 6 };

		// 1. 반복문
		for (int i = 0; i < nums.length; i++) {
			System.out.println(nums[i]);
		}

		// 2. 향상된 반복문 (readonly)
		for (int num : nums) {
			System.out.println(num);
		}
		
		// 3. 역방향 순회
	}
}
