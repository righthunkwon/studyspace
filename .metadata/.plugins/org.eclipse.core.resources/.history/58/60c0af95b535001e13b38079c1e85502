package array02;

import java.util.Scanner;

public class P_1966 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		// test case
		int tc = sc.nextInt();
		for (int t = 1; t <= tc; t++) {
			int n = sc.nextInt();
			int[] arr = new int[n]; 
			
			// 숫자열을 배열에 저장
			for (int i = 0; i < n; i++) {
				arr[i] = sc.nextInt();
			}
			
			// Counting Sort
			// 1. 배열의 최대값 구하기
			int k = -1; // 최대값 초기화
			for (int i = 0; i < n; i++) {
				if (k < arr[i]) {
					k = arr[i];
				}
			}
		}
	}
}
