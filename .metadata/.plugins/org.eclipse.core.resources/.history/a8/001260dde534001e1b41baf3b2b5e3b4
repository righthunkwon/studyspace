package array01;

import java.util.Scanner;

public class P_2063 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int middle = (n / 2) + 1;
		int[] nums = new int[n];

		// 배열 입력 반복문
		for (int i = 0; i < n; i++) {
			nums[i] = sc.nextInt();
		}

		// bubble sort(오름차순)
		// total cycle 	: nums.length - 1
		// each cycle 	: nums.length - 1++ (1, 2, 3, 4, ...)
		for (int j = 0; j < nums.length; j++) {
			for (int k = 1; k < nums.length - j; k++) {
				if (nums[k - 1] > nums[k]) {
					int tmp = nums[k];
					nums[k] = nums[k - 1];
					nums[k - 1] = tmp;
				}
			}
		}
	}
}
