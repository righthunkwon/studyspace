package array04;

import java.util.Scanner;

public class P_11315 {
	// 북 북동 동 동남 남 남서 서 서북
	static int[] dr = { -1, -1, 0, 1, 1, 1, 0, -1 };
	static int[] dc = { 0, 1, 1, 1, 0, -1, -1, -1 };

	static void changeDir(int dir) {
		dir = (dir + 1) % 4;
	}

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		// test case
		int tc = sc.nextInt();
		for (int t = 1; t <= tc; t++) {

			// 문자열 배열 입력
			int n = sc.nextInt();
			String[] sarr = new String[n];
			for (int i = 0; i < n; i++) {
				sarr[i] = sc.next();
			}

			// 문자 배열 입력
			char[][] carr = new char[n + 2][n + 2];
			for (int i = 1; i <= n; i++) {
				carr[i] = sarr[i].toCharArray();
			}

			// O 탐색
			int dir = 0; // 방향
			int r = 
			int cnt = 0;
			int ans = 0;
			for (int r = 1; r <= n; r++) {
				for (int c = 0; c<= n; c++) {
					if (carr[r][c] == 'O') {
						cnt++;
					}
				}
				if (cnt >= 5)
					ans++;
				if (ans >= 1) {
					System.out.printf("#%d %s\n", t, "YES");
					break;
				} else {
					System.out.printf("#%d %s\n", t, "NO");
				}
			}

		}

	}

}
