package array04;

import java.util.Scanner;

public class P_1954 {
	// 동 남 서 북 (달팽이 순회)
	static int[] dr = new int[] { 0, 1, 0, -1 };
	static int[] dc = new int[] { 1, 0, -1, 0 };
	static int[][] arr = new int[][];

	
	static int r = 0; // 현재 행 위치
	static int c = 0; // 현재 열 위치
	static int dir = 0; // 방향(0동, 1남, 2서, 3북)
	
	static void move(int moveDir) {
		r += dr[moveDir];
		c += dc[moveDir];
	}
	
	static boolean inRange(int r, int c, int n, int m) {
		return (0 <= r && r < n && 0 <= c && c < m && arr[r][c] == 0);
	}


	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		// test case
		int tc = sc.nextInt();
		for (int t = 1; t <= tc; t++) {
			
			// 배열 선언
			int n = sc.nextInt();
			int[][] arr = new int[n][n];
			
			// 달팽이 숫자 출력
			int val = 0; // 달팽이 숫자
			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					
				}
			}
			
			
			
			
			System.out.printf("#%d", t);
		}
	}
}
