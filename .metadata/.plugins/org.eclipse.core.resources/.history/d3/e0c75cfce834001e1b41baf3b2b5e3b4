package array01;

import java.util.Scanner;

public class P_1206 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int tc = 10;

		// test case
		for (int i = 0; i < tc; i++) {

			// 배열 입력 반복문
			for (int j = 0; j < nums.length; j++) {
				nums[j] = sc.nextInt();
			}
			System.out.print("#" + (i + 1) + " ");
		}
	}
}
