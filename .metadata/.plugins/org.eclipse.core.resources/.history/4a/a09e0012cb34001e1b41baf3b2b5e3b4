
public class Array01_03_BubbleSort {
	public static void main(String[] args) {
		// 오름차순 버블정렬
		int[] nums = { 24, 99999, 99, 31, 213124, 7, 35 };
		// 1번 사이클	   24   99    31 99999   7   35 213124 (213124는 정렬이 되었음을 확신)
		// 1번 사이클	   
		// 1번 사이클	   
		// 1번 사이클	   
		// 1번 사이클	   
		// 1번 사이클	   
		
		int N = nums.length; // 7
	}
}
