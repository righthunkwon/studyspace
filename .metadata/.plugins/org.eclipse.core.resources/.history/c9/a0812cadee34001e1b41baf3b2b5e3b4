package array01;

import java.util.Scanner;

public class P_gravity {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int width = sc.nextInt(); // 박스 가로 길이
		int[] height = new int[width]; // 박스 높이
		
		// 박스 높이 입력 반복문
		for (int i = 0; i < width; i++) {
			height[i] = sc.nextInt(); 
		}
	}

}
