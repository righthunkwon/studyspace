package array03;

public class Array03_01_BabyGin_완전탐색 {
	public static void main(String[] args) {
		int[] cards = { 3, 4, 7 };
		int n = 3;

		// 카드는 중복되지 않는다.
		// 첫번째 카드
		for (int i = 0; i < n; i++) {
			// 두번째 카드
			for (int j = 0; j < n; j++) {
				if (i != j) {
					// 세번째 카드
					for (int k = 0; k < n; k++) {
						if (k != i && k != j) {
							
						}
					}//3
				}//if
			}//2
		}//1
	}
}
