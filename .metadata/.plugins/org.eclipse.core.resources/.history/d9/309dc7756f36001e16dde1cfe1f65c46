package array03;

import java.util.Scanner;

public class P_1979 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		// test case
		int tc = sc.nextInt();
		for (int t = 1; t <= tc; t++) {
			int n = sc.nextInt(); // 배열 길이
			int k = sc.nextInt(); // 단어 길이
			int[][] arr = new int[n][n]; // n*n 배열

			// 배열 값 입력
			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					arr[i][j] = sc.nextInt();
				}
			}

			int cnt = 0; 			// 단어 길이 검사 변수
			int result = 0;

			// 배열 탐색(가로)
			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					// 조건 검사
					if (arr[i][j] == 1) {
						cnt++;
					} else if (cnt >= 1 && arr[i][j] == 0) {
						if (cnt == k) {
							totalCnt++;
						}
						cnt = 0;
					}
				}
				// 출력
				if (cnt == k) {
					totalCnt++;
				}
				cnt = 0;
			}

			// 배열 탐색(세로)
			for (int j = 0; j < n; j++) {
				for (int i = 0; i < n; i++) {
					// 조건 검사
					if (arr[i][j] == 1) {
						cnt++;
					} else if (cnt >= 1 && arr[i][j] == 0) {
						if (cnt == k) {
							totalCnt++;
						}
						cnt = 0;
					}
				}
				// 출력
				if (cnt == k) {
					totalCnt++;
				}
				cnt = 0;
			}
			System.out.printf("#%d %d\n", t, totalCnt);
		}
	}
}
