package array03;

import java.util.Scanner;

public class P_2001 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		// test case
		int tc = sc.nextInt();
		for (int t = 1; t <= tc; t++) {
			int n = sc.nextInt();
			int m = sc.nextInt();

			// 배열 값 입력
			int[][] arr = new int[n][n];
			for(int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					arr[i][j] = sc.nextInt();
				}
			}
			
			
			
			
			
		}
		
		
	}
}
