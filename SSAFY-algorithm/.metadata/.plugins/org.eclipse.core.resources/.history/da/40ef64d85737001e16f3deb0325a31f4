package array04;

import java.util.Scanner;

public class P_11315 {
	// 북 북동 동 동남 남 남서 서 서북
	static int[] dr = { -1, -1, 0, 1, 1, 1, 0, -1 };
	static int[] dc = { 0, 1, 1, 1, 0, -1, -1, -1 };

	static void changeDir(int dir) {
		dir = (dir + 1) % 4;
	}

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		// test case
		int tc = sc.nextInt();
		for (int t = 1; t <= tc; t++) {

			// 문자열 배열 입력
			int n = sc.nextInt();
			String[] sarr = new String[n];
			for (int i = 0; i < n; i++) {
				sarr[i] = sc.next();
			}

			// 문자 배열 입력
			char[][] carr = new char[n][n];
			for (int i = 1; i <= n; i++) {
				carr[i] = sarr[i].toCharArray();
			}

			// 탐색
			boolean find = false;
			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					for (int d = 0; d < dc.length; d++) {
						if (carr[i][j] == 'o') {
							int cnt = 1; // 시작 위치의 'o'를 세어야 하므로 1로 초기화
							int c = j;
							int r = i;

							while (c + dc[d] >= 0 && c + dc[d] < n && r + dr[d] >= 0 && r + dr[d] < n
									&& carr[r + dr[d]][c + dc[d]] == 'o') {
								cnt++;
								if (cnt >= 5) {
									find = true;
									break;
								}
								c += dc[d];
								r += dr[d];
							}
						}
					}
				}
			}
			System.out.printf("#%d %s\n", t, find ? "YES" : "NO");
		}
	}

}
