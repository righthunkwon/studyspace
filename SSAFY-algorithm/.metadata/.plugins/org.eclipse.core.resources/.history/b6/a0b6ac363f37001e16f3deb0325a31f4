package array04;

import java.util.Scanner;

public class P_1954 {
	// 오른쪽 아래쪽 왼쪽 위쪽
	static int[] dr = {0, 1, -1, 0};
	static int[] dc = {1, 0, 0, -1};
	
	// 방향 전환
	static int dir = 0;
	static void changeDir() {
		if (dir == 0) {
			dir = 1;
		} else if (dir == 1) {
			dir = 2;
		} else if (dir == 2) {
			dir = 3;
		} else if (dir == 3){
			dir = 0;
		}
	}
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		// test case
		int tc = sc.nextInt();
		for (int t = 1; t <= tc; t++) {
			// 배열 생성
			int r = 0; // 행
			int c = 0; // 열
			int n = sc.nextInt();
			int[][] arr = new int[n][n];
			
			// 배열에 값 집어넣기
			int cnt = 1; // 값
			while (cnt <= n*n) {
				arr[r][c] = cnt;
				int nr = r + dr[dir];
				int nc = c + dc[dir];
				if(0 <= nr && nr < n && 0 <= nc && nc < n && arr[nr][nc] == 0) {
					changeDir();
				}
				r += dr[dir];
				c += dc[dir];
				cnt++;
			}
			System.out.printf("#%d\n", t);
			for (int i = 0; i < n; i++) {
				for (int j = 0; j < n; j++) {
					System.out.print(arr[i][j]);
				}
				System.out.println();
			}
		}
	}
}
