package array04;

import java.util.Scanner;

public class P_11315 {
	// 북 북동 동 동남 남 남서 서 서북
	static int[] dr = { -1, -1, 0, 1, 1, 1, 0, -1 };
	static int[] dc = { 0, 1, 1, 1, 0, -1, -1, -1 };

	static void changeDir(int dir) {
		dir = (dir + 1) % 4;
	}

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		// test case
		int tc = sc.nextInt();
		for (int t = 1; t <= tc; t++) {

			// 문자열 배열 입력
			int n = sc.nextInt();
			String[] sarr = new String[n];
			for (int i = 0; i < n; i++) {
				sarr[i] = sc.next();
			}

			// 문자 배열 입력
			char[][] carr = new char[n][n];
			for (int i = 1; i <= n; i++) {
				carr[i] = sarr[i].toCharArray();
			}

			// 가로탐색 시작
			int r = 0;
			int c = 0;
			boolean find = false;
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < N; j++) {
					int count = 0;
					for (int d = 0; d < 5; d++) {
						int nr = i;
						int nc = j + dc1[d];
						if (nc < 0 || nc >= N)
							break;
						if (field[nr][nc] == 'o')
							count++;
					}
					if (count == 5)
						find = true;
				}
			}

			// 세로탐색 시작
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < N; j++) {
					int count = 0;
					for (int d = 0; d < 5; d++) {
						int nr = i + dr1[d];
						int nc = j;
						if (nr < 0 || nr >= N)
							break;
						if (field[nr][nc] == 'o')
							count++;
					}
					if (count == 5)
						find = true;
				}
			}

			// 대각선1 탐색 시작
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < N; j++) {
					int count = 0;
					for (int d = 0; d < 5; d++) {
						int nr = i + dr1[d];
						int nc = j + dc1[d];
						if (nr < 0 || nr >= N || nc < 0 || nc >= N)
							break;
						if (field[nr][nc] == 'o')
							count++;
					}
					if (count == 5)
						find = true;
				}
			}

			// 대각선2 탐색 시작
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < N; j++) {
					int count = 0;
					for (int d = 0; d < 5; d++) {
						int nr = i + dr1[d];
						int nc = j + dc2[d];
						if (nr < 0 || nr >= N || nc < 0 || nc >= N)
							break;
						if (field[nr][nc] == 'o')
							count++;
					}
					if (count == 5)
						find = true;
				}
			}
			System.out.printf("#%d %s\n", tc, find ? "YES" : "NO");

		}

	}

}
